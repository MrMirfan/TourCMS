{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Mvc": "Debug",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "54769af8-d507-424d-849b-5cf7e1c49159",
        "SanitizeTinyMce": true,
        "Smtp": {
          "From": "test@noreply.com",
          "Host": "smtp.gmail.com",
          "Port": 587,
          "Username": "username",
          "Password": "password"
        }
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Hosting": {
        "Debug": true
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false,
        "AllowPasswordReset": true
      }
    }
  }
}
